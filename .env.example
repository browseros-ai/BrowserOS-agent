# Ports
BROWSEROS_CDP_PORT=9000           # Server binds, Agent passes to BrowserOS
BROWSEROS_SERVER_PORT=9100        # Server HTTP endpoint
BROWSEROS_EXTENSION_PORT=9300     # Server WebSocket for extension
VITE_BROWSEROS_SERVER_PORT=9100   # Agent runtime (must = SERVER_PORT)

# BrowserOS config
BROWSEROS_CONFIG_URL=https://llm.browseros.com/api/browseros-server/config
BROWSEROS_BINARY=/Applications/BrowserOS.app/Contents/MacOS/BrowserOS
BROWSEROS_VERSION=
BROWSEROS_INSTALL_ID=
BROWSEROS_CLIENT_ID=

# Agent build
USE_BROWSEROS_BINARY=true
BROWSEROS_DISABLE_SERVER=true
BROWSEROS_USER_DATA_DIR=/tmp/browseros-dev

# Telemetry (server)
POSTHOG_API_KEY=
POSTHOG_ENDPOINT=
SENTRY_DSN=

# Telemetry (browser)
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=
VITE_PUBLIC_SENTRY_DSN=

# Sentry build
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

NODE_ENV=development
LOG_LEVEL=info
